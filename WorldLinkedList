public class WorldLinkedList {
    Node head;
    public void insertAtEnd(String data)
    {
        Node node = new Node();
        node.data = data;
        node.next = null;
        if(head == null)
        {
            head = node;
        }
        else
        {
            Node temp = head;
            while(temp.next != null)
            {
                temp = temp.next;
            }
            temp.next = node;
        }
    }
    public void displayAll()
    {
        Node node = head;
        while(node.next!=null)
        {
            System.out.println(node.data);
            node = node.next;
        }
        System.out.println(node.data);
    }
    public void insertAtBegining(String data)
    {
        Node node = new Node();
        node.data = data;
        node.next = null;
        node.next = head;
        head = node;
    }
    public void insertAtPosition(int index, String data)
    {
        Node node = new Node();
        node.data = data;
        node.next = null;
        Node n = head;
        if(index == 0)
        {
            insertAtBegining(data);
        }
        for(int i = 0; i <index-1; i++)
        {
            n=n.next;
        }
        node.next = n.next;
        n.next = node;
    }
    public void deleteFromBegining()
    {
        head = head.next;   
    }
    public void deleteAtPosition(int index)
    {
        Node n = head;
        Node n1 = null;
        if(index == 0)
        {
            head = head.next;
        }
        for(int i = 0; i <index-1; i++)
        {
            n=n.next;
        }
        n1 = n.next;
        n.next = n1.next;
        System.out.println(n1.data + " was deleted from the list.");
        
        
    }
}
